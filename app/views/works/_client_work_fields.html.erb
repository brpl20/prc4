<div class='control-group nested-fields'>
  <div class="controls">
    <div class="col-md-10 no-padding">
      <%= f.select :client_id, options_for_client, {prompt: 'Selecione ou digite um nome de um cliente'}, {class: "client-select form-control"} %>
    </div>
    <div class="col-md-2 no-padding">
      <%= link_to_remove_association("X", f, class:"adj100 btn btn-outline btn-danger btn-remove") %>
    </div>
  </div>
</div>
