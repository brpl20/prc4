<p id="notice"><%= notice %></p>



<h1>Clients</h1>

<table>
  <thead>
    <tr>
      <th>Genero</th>
      <th>Nome</th>
      <th>Sobrenome</th>
      <th>Nacionalidade</th>
      <th>Estadocivil</th>
      <th>Capacidade</th>
      <th>Profissao</th>
      <th>Empresa atual</th>
      <th>Nascimento</th>
      <th>Mae</th>
      <th>Nb</th>
      <th>Rg</th>
      <th>Cpf</th>
      <th>Email</th>
      <th>Endereco</th>
      <th>Cidade</th>
      <th>Estado</th>
      <th>Banco</th>
      <th>Agencia</th>
      <th>Conta</th>
      <th>Cep</th>
      <th>Telefone</th>
      <th>Documentos</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @clients.each do |client| %>
      <tr>
        <td><%= client.gender %></td>
        <td><%= client.name %></td>
        <td><%= client.lastname %></td>
        <td><%= client.citizenship %></td>
        <td><%= client.civilstatus %></td>
        <td><%= client.capacity %></td>
        <td><%= client.profession %></td>
        <td><%= client.company %></td>
        <td><%= client.birth %></td>
        <td><%= client.mothername %></td>
        <td><%= client.number_benefit %></td>
        <td><%= client.general_register %></td>
        <td><%= client.social_number %></td>
        <td><%= client.email %></td>
        <td><%= client.address %></td>
        <td><%= client.city %></td>
        <td><%= client.state %></td>
        <td><%= client.bank %></td>
        <td><%= client.agency %></td>
        <td><%= client.account %></td>
        <td><%= client.zip %></td>
        <td><%= client.telephone %></td>
        <td><%= client.documents %></td>
        <td><%= link_to 'Show', client %></td>
        <td><%= link_to 'Edit', edit_client_path(client) %></td>
        <td><%= link_to 'Destroy', client, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

