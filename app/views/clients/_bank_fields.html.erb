  <div class="col-md-12">
    <label>Banco</label>
    <%= f.collection_select :name, options_for_banks_conc, :first, :second, {:include_blank => 'Escolha um banco'}, {class:'form-control'}%>
  </div>

  <div class="col-md-6">
    <label>Agência</label>
    <%= f.text_field :agency, placeholder:"Agência", class:"form-control" %>
  </div>

  <div class="col-md-6">
    <label>Conta</label>
    <%= f.text_field :account, placeholder:"Conta", class:"form-control" %>
  </div>
