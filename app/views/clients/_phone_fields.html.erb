<div class='control-group nested-fields'>
  <div class="controls">
    <div class="col-md-10 no-padding">
      <%= f.text_field :phone, class:"form-control phone",'data-mask': '(00) 00000-0000', placeholder:"(00) 0 0000-0000" %>
    </div>
    <div class="col-md-2 no-padding">
      <%= link_to_remove_association "X", f, class:"btn btn-anger btn-rmv-cli" %>
    </div>
  </div>
</div>
