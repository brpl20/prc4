<p id="notice"><%= notice %></p>

<h1>People</h1>

<table>
  <thead>
    <tr>
      <th>Address</th>
      <th>Birth</th>
      <th>Capacity</th>
      <th>Citizenship</th>
      <th>City</th>
      <th>Civilstatus</th>
      <th>Company</th>
      <th>Email</th>
      <th>First name</th>
      <th>Lastname</th>
      <th>Gender</th>
      <th>General register</th>
      <th>Mothername</th>
      <th>Number benefit</th>
      <th>Oab number</th>
      <th>Profession</th>
      <th>Social number</th>
      <th>State</th>
      <th>Zip</th>
      <th>Status</th>
      <th>Life</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @people.each do |person| %>
      <tr>
        <td><%= person.address %></td>
        <td><%= person.birth %></td>
        <td><%= person.capacity %></td>
        <td><%= person.citizenship %></td>
        <td><%= person.city %></td>
        <td><%= person.civilstatus %></td>
        <td><%= person.company %></td>
        <td><%= person.email %></td>
        <td><%= person.first_name %></td>
        <td><%= person.lastname %></td>
        <td><%= person.gender %></td>
        <td><%= person.general_register %></td>
        <td><%= person.mothername %></td>
        <td><%= person.number_benefit %></td>
        <td><%= person.oab_number %></td>
        <td><%= person.profession %></td>
        <td><%= person.social_number %></td>
        <td><%= person.state %></td>
        <td><%= person.zip %></td>
        <td><%= person.status %></td>
        <td><%= person.life %></td>
        <td><%= link_to 'Show', person %></td>
        <td><%= link_to 'Edit', edit_person_path(person) %></td>
        <td><%= link_to 'Destroy', person, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Person', new_person_path %>
